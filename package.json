{
  "name": "paletten",
  "version": "0.0.21-beta",
  "description": "Simple color palette tokens generator",
  "main": "./build/esm/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc -d -p ./tsconfig.cjs.json & tsc -d -p ./tsconfig.esm.json",
    "assetgen": "node --experimental-specifier-resolution=node ./__dev__/assetgen/main.js"
  },
  "exports": {
    ".": {
      "browser": "./build/esm/index.js",
      "umd": "./build/esm/index.js",
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otsubocloud/paletten.git"
  },
  "author": "yoshimasa otsubo",
  "license": "MIT",
  "homepage": "https://github.com/otsubocloud/paletten#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
